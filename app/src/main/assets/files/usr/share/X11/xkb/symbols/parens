// swap parentheses and square brackets
default partial
xkb_symbols "swap_brackets" {
    replace key <AE09> {        [         9,    bracketleft     ]       };
    replace key <AE10> {        [         0,    bracketright    ]       };
    replace key <AD11> {        [ parenleft,    braceleft       ]       };
    replace key <AD12> {        [ parenright,   braceright      ]       };
};
